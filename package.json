{
  "name": "bob-bundler",
  "version": "0.4.0",
  "description": "Bundle HTML, CSS, JS with optional server and live reload",
  "main": "./build/index.js",
  "bin": {
    "bob": "bin/bob"
  },
  "scripts": {
    "prebuild": "mkdir -p build",
    "build": "babel src --out-dir build",
    "dev": "babel src --watch --out-dir build",
    "clean": "rm -rf build",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "chalk": "^1.1.3",
    "fs-extra": "^2.0.0",
    "glob": "^7.1.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-bro": "^1.0.1",
    "gulp-clean-css": "^3.0.0",
    "gulp-ejs": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.10",
    "minimist": "^1.2.0",
    "node-static": "^0.7.9",
    "nodemon": "^1.11.0",
    "readline-sync": "^1.4.4",
    "ws": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.4.0"
  },
  "author": "Eliot Akira",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliot-akira/bob-bundler.git"
  },
  "license": "MIT"
}
