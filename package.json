{
  "name": "bob-bundler",
  "version": "0.0.1",
  "description": "Opinionated bundler of JavaScript projects",
  "main": "./build/index.js",
  "bin": {
    "bob": "bin/bob"
  },
  "scripts": {
    "prebuild": "mkdir -p build",
    "build": "babel src --out-dir build",
    "dev": "babel src --watch --out-dir build",
    "clean": "rm -rf build",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "chalk": "^1.1.3",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-bro": "^0.3.1",
    "gulp-clean-css": "^2.0.13",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.10",
    "minimist": "^1.2.0",
    "nodemon": "^1.11.0",
    "readline-sync": "^1.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.4.0"
  },
  "unused": {
    "gulp-ejs": "^2.2.0",
    "node-static": "^0.7.8",
    "ws": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliot-akira/bob-bundler.git"
  }
}
