{
  "name": "bob-example-client-react",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "bob dev",
    "build": "bob build"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-preset-react": "^6.16.0",
    "bob-bundler": "*"
  },
  "bob": {
    "browserify": {
      "src": "src/index.js",
      "dest": "build/app.js",
      "watch": "src/**/*.js"
    },
    "sass": {
      "src": "src/index.scss",
      "dest": "build/app.css",
      "watch": "src/**/*.scss"
    },
    "ejs": {
      "src": "src/index.html",
      "dest": "build",
      "watch": "src/**/*.html"
    },
    "static": {
      "src": "build",
      "port": 3000
    }
  }
}
